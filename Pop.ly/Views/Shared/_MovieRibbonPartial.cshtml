@model IEnumerable<Pop.ly.Models.Database.Movie>

<hr />
<section class="MovieRibbon mt-2">
    @foreach (var movie in Model)
    {
        <div class="RibbonContent">
            <a href="/Movie/?T=@movie.Title&Y=@movie.ReleaseYear"><img src="@movie.CoverArt" /></a>
        </div>
    }
</section>
<hr />
